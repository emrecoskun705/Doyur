<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Site1.master.cs" Inherits="Doyur.product.Site1" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title>Ürün</title>
    <link href="/css/site.css" rel="stylesheet" />
    <link href="/css/restaurantproduct.css" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
    <script src="/js/alert.js"></script>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <form id="form1" runat="server">
        <div>
            <div class="my-navbar">
                <ul>
                    <li><a class="" href="/default.aspx">Anasayfa</a></li>
                    <%
                    if(IT.Session.Users.UserId() == 0)
                    {

                    %>
                        <li style="float:right"><a href="/login.aspx">Giriş</a></li>
                        <li style="float:right"><a href="/register.aspx">Kayıt Ol</a></li>
                    <%
                    } else
                    {
                    %>
                    <li style="float:right"><a href="/logout.aspx">Çıkış Yap</a></li>
                    <li style="float:right"><a href="/user/default.aspx">Kullanıcı</a></li>
                    <%
                        }
                    %>

                    <% 
                    if (IT.Session.Users.AccessId() == 2)
                    { 
                    %>
                    <li style="float:right"><a href="/order/default.aspx">
                        <img src="/image/icons8-shopping-basket-16%20(1).png" /> </a></li>
                    <%  
                    } 
                    %>

                    <% 
                    if (IT.Session.Users.AccessId() == 3)
                    { 
                    %>
                    <li style="float:right"><a href="/company/default.aspx">Şirket Paneli</a></li>
                    <%  
                    } 
                    %>

                    <% 
                    if (IT.Session.Users.AccessId() == 255)
                    { 
                    %>
                    <li style="float:right"><a href="/admin/default.aspx">Admin Paneli</a></li>
                    <%  
                    } 
                    %>

                    
                    <!-- <li style="float:right"><a href="user/default.aspx">Kullanıcı</a></li> -->
                </ul>
                <div id="alert-message"></div>
            </div>
            <br /><br /><br />  
            <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
            </asp:ContentPlaceHolder>
        </div>
    </form>
</body>
</html>
